{
    "name": "optimistdigital/nova-resizable",
    "description": "Simple Laravel Nova tool to enable column resizing",
    "keywords": [
        "optimistdigital",
        "laravel",
        "nova",
        "resizing",
        "columns",
        "table"
    ],
    "homepage": "https://github.com/optimistdigital/nova-resizable",
    "license": "MIT",
    "authors": [
        {
            "name": "Kaspar Rosin",
            "email": "kaspar@optimistdigital.com",
            "homepage": "https://optimistdigital.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2.0"
    },
    "autoload": {
        "psr-4": {
            "OptimistDigital\\NovaResizable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OptimistDigital\\NovaResizable\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
